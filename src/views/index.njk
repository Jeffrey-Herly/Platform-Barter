<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Webix Example</title>
  <script src="https://cdn.webix.com/edge/webix.js"></script>
  <link rel="stylesheet" href="https://cdn.webix.com/edge/webix.css">
</head>
<body>
  <div id="webix-container" style="width:100%; height:100%;"></div>
  <script>
    webix.ui({
      container: "webix-container",
      rows: [
        { view: "toolbar", elements: [
          { view: "label", label: "Webix Toolbar" },
          { view: "button", value: "Click Me", width: 100, click: function() {
              webix.message("Button clicked!");
            }
          }
        ]},
        { view: "datatable", autoConfig: true, data: [
          { id: 1, name: "John Doe", age: 30 },
          { id: 2, name: "Jane Smith", age: 25 },
          { id: 3, name: "Sam Johnson", age: 35 }
        ]}
      ]
    });
  </script>
</body>
</html>